<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>main.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addTeam</servlet-name>
        <servlet-class>com.ourlives.servlets.Search</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addTeam</servlet-name>
        <url-pattern>/AddTeam.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>aboutus</servlet-name>
        <jsp-file>AboutUs.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>aboutus</servlet-name>
        <url-pattern>/secdscl</url-pattern>
    </servlet-mapping>
</web-app>
